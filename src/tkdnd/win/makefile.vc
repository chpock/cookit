	"$(INTDIR)\tkDNDBind.obj" \
	"$(INTDIR)\tkOleDND.obj" \
	"$(INTDIR)\tkOleDND_TEnumFormatEtc.obj" \
	"$(INTDIR)\tkShape.obj" \
	"$(INTDIR)\Version.res"

"..\lib\tkDND\libtkdnd.dll" : "$(OUTDIR)" $(DEF_FILE) $(LINK32_OBJS)
    $(LINK32) @<<
  $(LINK32_FLAGS) $(LINK32_OBJS)
<<

